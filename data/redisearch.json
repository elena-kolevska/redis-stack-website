{
    "id": "redisearch",
    "module_id": "search",
    "type": "module",
    "name": "Search",
    "description": "Secondary index operations",
    "stack_path": "search",
    "stack_weight": 10,
    "repository": {
        "git_uri": "https://github.com/redisearch/redisearch"
    },
    "commands": {
        "git_uri": "https://github.com/redisearch/redisearch",
        "dev_branch": "master",
        "branches_postfix": "-newdocs",
        "defs": "commands.json",
        "path": "docs/commands"
    },
    "docs": {
        "git_uri": "https://github.com/redisearch/redisearch",
        "dev_branch": "master",
        "branches_postfix": "-newdocs",
        "path": "docs/docs"
    },
    "releases": {
        "source": "docs",
        "tags": true,
        "include_tag_regex": "v([0-9]+\\.[0-9]+\\.[0-9]+)",
        "exclude_tag_regexes": [],
        "include_branch_regex": "([0-9]+\\.[0-9])",
        "exclude_branch_regexes": []
    }
}